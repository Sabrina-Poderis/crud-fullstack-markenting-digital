{
  "name": "fullstack-markenting-digital-frontend",
  "private": true,
  "version": "1.0.4",
  "description": "O sistema deve permitir o gerenciamento de usuários e projetos de clientes de marketing digital, com comunicação via API REST",
  "main": "index.js",
  "keywords": ["node_v18","vue", "typescript"],
  "author": "sabrina.poderis@gmail.com",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint --fix --ext .js,.vue src",
    "format": "prettier --write \"src/**/*.{js,vue,ts,css,scss,json,md}\"",
    "docker:build": "docker build -t frontend .",
    "docker:start": "docker run -p 8080:80 frontend"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "date-fns": "^4.1.0",
    "jwt-decode": "^4.0.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.14",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.5",
    "eslint-plugin-vue": "^10.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2",
    "vite": "^6.2.0",
    "vue-tsc": "^2.2.4"
  }
}
